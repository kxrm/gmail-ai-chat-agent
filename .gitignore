# Gmail AI Chat Agent - .gitignore

# ====================
# PHP & Composer
# ====================
vendor/
composer.phar
composer.lock.backup

# ====================
# Configuration & Secrets  
# ====================
# Google OAuth credentials (contains client secrets)
config/client_secret.json
# Backup config files
config/*.backup
config/*.bak

# ====================
# Runtime & Storage
# ====================
# Session files (user sessions contain sensitive data)
storage/sessions/sess_*
!storage/sessions/.gitkeep
# Temporary session files
storage/sessions/temp_sessions/sess_*
!storage/sessions/temp_sessions/
!storage/sessions/temp_sessions/.gitkeep

# Log files (may contain sensitive information)
storage/logs/*
!storage/logs/.gitkeep

# Cache and temporary files
storage/cache/
storage/tmp/
tmp/
temp/

# ====================
# Testing & Coverage
# ====================
# Generated coverage reports
coverage-report/
build/
# PHPUnit cache
.phpunit.result.cache
# Test output logs
test-output.log
tests/**/output/

# Infection mutation testing
infection.log

